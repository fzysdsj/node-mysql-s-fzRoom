                        <script>
                            $(function() {
                                $("#like").click(function() {
                                    let artId = <%= datas[0].artId %>;
                                    let url = "/articles/ajax/" + artId;
                                    let user = parseInt($("#userId").text().trim());
                                    let likeNumber = parseInt($("#likeNumber").text().trim());
                                    likeNumber += 1;
                                    $("#likeNumber").text(likeNumber);
                                    let likeId = $("#like").attr("id");
                                    $.ajax({
                                        url: url,
                                        type: "GET",
                                        dataType: "json",
                                        data: {
                                            like: likeId,
                                            user: user
                                        },
                                        contentType: "application/json",
                                        cache: false,
                                        timeout: 5000,
                                        complete: function() {
                                            //called when complete
                                            console.log('process complete');
                                        },
                                        success: function(data) {

                                            console.log(data);
                                            console.log('process sucess');
                                            alert("喜欢就好");
                                            $("#like").addClass('hidden');
                                            $("#do-like").removeClass('hidden');
                                        },

                                        error: function() {
                                            console.log('process error');
                                        }
                                    });
                                });
                                $("#back").click(function() {
                                    let artId = <%= datas[0].artId %>;
                                    let url = "/articles/ajax/art/" + artId;
                                    $.ajax({
                                        url: url,
                                        type: "GET",
                                        dataType: "json",
                                        data: {},
                                        contentType: "application/json",
                                        cache: false,
                                        timeout: 5000,
                                        complete: function() {
                                            //called when complete
                                            console.log('process complete');
                                        },
                                        success: function(data) {

                                            console.log(data);
                                            console.log('process sucess');
                                            let backString = "上一篇:" + data.backTitle + "(" + data.backCategory + ")";
                                            let backUrl = "/articles/art/" + data.backId;
                                            $("#back").text(backString);
                                            $("#back").attr('href', backUrl);
                                            $("#back").attr('data-content', "再点一次转到上一篇");
                                        },

                                        error: function() {
                                            console.log('process error');
                                        }
                                    });
                                });
                                $("#next").click(function() {
                                    let artId = <%= datas[0].artId %>;
                                    let url = "/articles/ajax/art/" + artId;
                                    $.ajax({
                                        url: url,
                                        type: "GET",
                                        dataType: "json",
                                        data: {},
                                        contentType: "application/json",
                                        cache: false,
                                        timeout: 5000,
                                        complete: function() {
                                            //called when complete
                                            console.log('process complete');
                                        },
                                        success: function(data) {
                                            console.log(data);
                                            console.log('process sucess');
                                            let nextString = "下一篇:" + data.nextTitle + "(" + data.nextCategory + ")";
                                            let nextUrl = "/articles/art/" + data.nextId;
                                            $("#next").text(nextString);
                                            $("#next").attr('href', nextUrl);
                                            $("#next").attr('data-content', "再点一次转到下一篇");
                                        },
                                        error: function() {
                                            console.log('process error');
                                        }
                                    });
                                });
                                $("#tip").click(function() {
                                    let artId = <%= datas[0].artId %>;
                                    let url = "/articles/ajax/tip/" + artId;
                                    let user = parseInt($("#userId").text().trim());
                                    $.ajax({
                                        url: url,
                                        type: "GET",
                                        dataType: "json",
                                        data: {
                                            tipNumber: 10
                                        },
                                        contentType: "application/json",
                                        cache: false,
                                        timeout: 5000,
                                        complete: function() {
                                            //called when complete
                                            console.log('process complete');
                                        },
                                        success: function(data) {

                                            console.log(data);
                                            console.log('process sucess');
                                            alert("谢大爷赏！");
                                            $("#tip").addClass('hidden');
                                            $("#do-tip").removeClass('hidden');
                                        },

                                        error: function() {
                                            console.log('process error');
                                        }
                                    });
                                });
                            })
                        </script>
